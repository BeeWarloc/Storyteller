<?xml version="1.0"?>
<CommandLineApplicationReport xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ApplicationName>st</ApplicationName>
  <Commands>
    <CommandReport>
      <Name>run</Name>
      <Description>Run a suite of StoryTeller tests</Description>
      <Arguments>
        <ArgumentReport>
          <Name>path</Name>
          <Description>Path to the StoryTeller project file or the project directory</Description>
        </ArgumentReport>
        <ArgumentReport>
          <Name>resultspath</Name>
          <Description>Path to write out the results</Description>
        </ArgumentReport>
      </Arguments>
      <Flags>
        <FlagReport>
          <UsageDescription>[-w, --workspace &lt;workspace&gt;]</UsageDescription>
          <Description>Optional.  Runs only one workspace</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-c, --compile &lt;compile&gt;]</UsageDescription>
          <Description>Specify a compile target</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-p, --profile &lt;profile&gt;]</UsageDescription>
          <Description>Storyteller test mode profile for systems like Serenity that use this</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-l, --lifecycle Acceptance|Regression|Any]</UsageDescription>
          <Description>Optional. Only runs tests with desired lifecyle</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-m, --max-retry &lt;maxretry&gt;]</UsageDescription>
          <Description>Optional. Will not retry tests more than max retry limit.</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Execute</Description>
          <Usage>st run &lt;path&gt; [-w, --workspace &lt;workspace&gt;] [-c, --compile &lt;compile&gt;] [-p, --profile &lt;profile&gt;] [-l, --lifecycle Acceptance|Regression|Any] [-m, --max-retry &lt;maxretry&gt;]</Usage>
        </UsageReport>
        <UsageReport>
          <Description>Execute and save results</Description>
          <Usage>st run &lt;path&gt; &lt;resultspath&gt; [-w, --workspace &lt;workspace&gt;] [-c, --compile &lt;compile&gt;] [-p, --profile &lt;profile&gt;] [-l, --lifecycle Acceptance|Regression|Any] [-m, --max-retry &lt;maxretry&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
    <CommandReport>
      <Name>specs</Name>
      <Description>Dumps the html preview for documentation efforts</Description>
      <Arguments>
        <ArgumentReport>
          <Name>path</Name>
          <Description>Path to the StoryTeller project file or the project directory</Description>
        </ArgumentReport>
        <ArgumentReport>
          <Name>resultspath</Name>
          <Description>Path to write out the results</Description>
        </ArgumentReport>
      </Arguments>
      <Flags>
        <FlagReport>
          <UsageDescription>[-t, --title &lt;title&gt;]</UsageDescription>
          <Description>Descriptive title for the html summary</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-o, --open]</UsageDescription>
          <Description>Open the index file after generation</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-w, --workspace &lt;workspace&gt;]</UsageDescription>
          <Description>Optional.  Runs only one workspace</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-c, --compile &lt;compile&gt;]</UsageDescription>
          <Description>Specify a compile target</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-p, --profile &lt;profile&gt;]</UsageDescription>
          <Description>Storyteller test mode profile for systems like Serenity that use this</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-l, --lifecycle Acceptance|Regression|Any]</UsageDescription>
          <Description>Optional. Only runs tests with desired lifecyle</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-m, --max-retry &lt;maxretry&gt;]</UsageDescription>
          <Description>Optional. Will not retry tests more than max retry limit.</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Dumps the html preview for documentation efforts</Description>
          <Usage>st specs &lt;path&gt; &lt;resultspath&gt; [-t, --title &lt;title&gt;] [-o, --open] [-w, --workspace &lt;workspace&gt;] [-c, --compile &lt;compile&gt;] [-p, --profile &lt;profile&gt;] [-l, --lifecycle Acceptance|Regression|Any] [-m, --max-retry &lt;maxretry&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
    <CommandReport>
      <Name>validate</Name>
      <Description>Validate that all usages of fixtures and grammars are valid within a suite of StoryTeller tests.</Description>
      <Arguments>
        <ArgumentReport>
          <Name>path</Name>
          <Description>Path to the StoryTeller project file or the project directory</Description>
        </ArgumentReport>
      </Arguments>
      <Flags>
        <FlagReport>
          <UsageDescription>[-c, --compile &lt;compile&gt;]</UsageDescription>
          <Description>Specify a compile target</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Validate that all usages of fixtures and grammars are valid within a suite of StoryTeller tests.</Description>
          <Usage>st validate &lt;path&gt; [-c, --compile &lt;compile&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
  </Commands>
</CommandLineApplicationReport>